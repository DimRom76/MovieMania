{{#each this}}
<div data-id={{id}} class="card">
    <a class="card__link" href="/#/movie/{{id}}">
        <img class="card-img-top" src="{{poster_path}}" data-id={{id}} alt={{title}} />
        <h5 data-id={{id}}>{{title}}</h5>
        <p>{{release_date}}</p>
        <p class="card-text" data-id={{id}}><i class="material-icons icon-raiting">star_rate</i>{{vote_average}}</p>

        <div class="btn-group btn-group-sm" role="group">
            <p data-id={{id}}><i class="material-icons favorites">favorite_border</i></p>
            <p data-id={{id}}><i class="material-icons watch__later">bookmark_border</i></p>
            <p data-id={{id}}><i class="material-icons watched">check_circle_outline</i></p>
        </div>
    </a>
</div>
{{/each}}