{{#each this}}
<div data-id={{id}} class="card">
    <a class="card__link" href="/#/movie/{{id}}">
        <img class="card-img-top" src="{{poster_path}}" data-id={{id}} alt={{title}} />
    </a>
    <h5 class="card-main-title" data-id={{id}}>{{title}}</h5>
    <div class="card-release-vote">
        <p class="text-release-vote">{{release_date}}</p>
        <p class="card-text text-release-vote" data-id={{id}}><i
                class="material-icons icon-raiting">star_rate</i>{{vote_average}}</p>
    </div>

    <div class="btn-group btn-group-sm status__film__js" data-container={{id}} role="group">
    </div>
</div>
{{/each}}
